{
  "name": "handbrake-js",
  "author": "Lloyd Brookes <75pound@gmail.com>",
  "version": "1.0.0-alpha.7",
  "description": "Handbrake for node.js. Brings video encoding.",
  "main": "./lib/handbrake-js.js",
  "bin": {
    "handbrake": "bin/cli.js"
  },
  "keywords": [
    "handbrake",
    "encode",
    "transcode",
    "video"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "files": ["bin/cli.js", "lib", "scripts"],
  "scripts": {
    "test": "tape test/*.js",
    "ubuntu-setup": "./scripts/install-ubuntu.sh",
    "postinstall": "node scripts/install.js",
    "lint": "jshint lib/*.js scripts/*.js bin/*.js test/*.js; echo",
    "boil": "baldrick --do tmp/balders.sh --when lib/*.js docs/*.hbs ../handlebars-array/lib/*.js ../jsdoc-to-markdown/jsdoc-template/publish.js ../jsdoc-to-markdown/lib/*.js ../jsdoc-to-markdown/partials/*.hbs --change"
  },
  "repository": "https://github.com/75lb/handbrake-js.git",
  "dependencies": {
    "console-dope": "~0.3.0",
    "more-fs": "~0.3.0",
    "nature": "~0.4.0",
    "request": "~2.34",
    "unzip": "~0.1"
  },
  "devDependencies": {
    "tape": "^2.12.3"
  }
}
