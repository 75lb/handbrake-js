{
  "name": "handbrake-js",
  "author": "Lloyd Brookes",
  "version": "0.3.2",
  "description": "An alternative, cross-platform distrubution of HandbrakeCLI (v0.9.9) adding javascript, streaming and improved command-line interfaces.",
  "main": "./lib/handbrake-js.js",
  "bin": {
    "handbrake": "bin/cli.js"
  },
  "keywords": [
    "handbrake",
    "encode",
    "transcode",
    "video"
  ],
  "engines": { "node": ">=0.10.0" },
  "scripts": {
    "test": "tape test/*.js",
    "ubuntu-setup": "./scripts/ubuntu-setup.sh",
    "postinstall": "./scripts/install-handbrakeCLI.js",
    "lint": "jshint lib/*.js scripts/*.js bin/*.js test/*.js; echo"
  },
  "repository": "https://github.com/75lb/handbrake-js.git",
  "dependencies": {
    "console-dope": "~0.3.0",
    "more-fs": "~0.3.0",
    "nature": "~0.4.0",
    "request": "~2.34",
    "unzip": "~0.1"
  },
  "devDependencies": {
    "tape": "^2.12.3"
  }
}
