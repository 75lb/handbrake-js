{
  "name": "handbrake-js",
  "author": "Lloyd Brookes",
  "version": "0.0.3",
  "description": "A reliable, self-sufficient, cross-platform package for HandbrakeCLI designed to bless Node.js projects with video encoding skills.",
  "license": "MIT",
  "main": "./lib/handbrake.js",
  "bin": "cli.js",
  "keywords": [
    "handbrake",
    "handbraker",
    "batch",
    "encode",
    "transcode",
    "video",
    "mp4",
    "m4v",
    "mkv",
    "avi",
    "mpg",
    "divx",
    "xvid"
  ],
  "scripts": {
    "test": "mocha --reporter spec test/*.js",
    "ubuntu-setup": "scripts/ubuntu-setup.sh",
    "postinstall": "node scripts/install-handbrakeCLI.js"
  },
  "repository": "https://github.com/75lb/handbrake-js.git",
  "dependencies": {
    "fs-extra": "~0.5",
    "request": "~2.16",
    "unzip": "~0.1",
    "nature": "~0.0.1"
  },
  "devDependencies": {
    "mocha": ">=1.7"
  }
}
